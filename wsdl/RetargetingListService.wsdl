        </sequence>
      </complexType>
      <complexType name="RuleBaseTargetList">
        <complexContent>
          <extension base="tns:TargetingList">
            <sequence>
              <element maxOccurs="20" minOccurs="0" name="rules" type="tns:RuleGroup"/>
