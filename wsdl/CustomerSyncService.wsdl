        </sequence>
      </complexType>
      <simpleType name="IncludeUnset">
        <restriction base="xsd:string">
          <enumeration value="INCLUDED"/>
          <enumeration value="EXCLUDE"/>
        </restriction>
      </simpleType>
      <complexType name="CustomerChangeData">
        <complexContent>
          <extension base="tns:Page">
            <sequence>
